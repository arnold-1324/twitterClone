{
  "version": 2,
  "builds": [
    {
      "src": "Frontend/package.json",
      "use": "@vercel/static-build",
      "config": { "distDir": "Frontend/dist" }
    }
  ],
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "https://twitterclone-backend-681i.onrender.com/api/:path*"
    },
    {
      "source": "/(.*)",
      "destination": "/Frontend/index.html"
    }
  ]
}


