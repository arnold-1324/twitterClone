{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": { "distDir": "dist" }
    }
  ],
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "https://twitterclone-backend-681i.onrender.com/api/:path*"
    }
  ],
  "routes": [
    
    { "handle": "filesystem" },
    
    { "src": "/(.*)", "dest": "/index.html" }
  ]
}
